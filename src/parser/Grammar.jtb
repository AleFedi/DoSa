 */options{  JDK_VERSION = "1.5";  static = true;  JTB_P = "parser";}PARSER_BEGIN(DoSaParser)package parser;import parser.visitor.*;public class DoSaParser{  public static void main(String args [])  {    System.out.println("Inserire i dati");    new DoSaParser(System.in);    try    {      Scope scope = DoSaParser.Scope();      DepthFirstVoidVisitor v = new MyVisitor();      scope.accept(v);    }    catch (Exception e)    {      System.out.println("Oops.");      System.out.println(e.getMessage());    }  }}class MyVisitor extends DepthFirstVoidVisitor{  public void visit(NodeToken n)  {    System.out.println("visit " + n.tokenImage);  }}PARSER_END(DoSaParser)SKIP :{  " "| "\t"| "\r"| "\n"| "\f"}TOKEN :{  < SESSO :    "M"  | "m"  | "F"  | "f" >| < TIPO :    "Socio Donatore"  | "Socio Benemerito"  | "Occasionale"  | "Socio Collaboratore" >| < GRUPPOSANGUIGNO :    "A"  | "B"  | "AB"  | "0"  | "NS" >| < RH :    "POSITIVO"  | "NEGATIVO"  | "NS" >| < IDONEITA :    "I"  | "N" >| < TIPODONAZIONE :    "Multicomponent"  | "Plasmaferesi"  | "Piastrinoaferesi"  | "Sangue Intero" >| < UNITARACCOLTA : "Unità Raccolta Fissa" >| < EMAIL :    (      < ID >    | "-"    | "_"    | "."    | < NUM >    )*    "@" (< ID > | "." )* < ID >>| < IDENTIFICATOR : < ID > (" " < ID >)* >| < ID : [ "A"-"Z", "a"-"z" ] ([ "A"-"Z", "a"-"z" ])* >| < NUM : [ "0"-"9" ] ([ "0"-"9" ])* >}void Scope() :{}{  (    Gruppo()  | Donatore()  | Donazione()  )*
  "."}void Gruppo() :{}{  "***GRUPPO DONATORI***" Nome() "- NUMERO:" < NUM > "- DATA FONDAZIONE:" Data() Indirizzo() EMail() "- PRESIDENTE:" Nome() "-" Cognome() "- SEGRETARIO:" Nome() "-" Cognome() "- NUMERO ISCRITTI:" < NUM > [ "- SEZIONE:" Sezione() ("-" Sezione())* ] ";"}void Donatore() :{}{  "***DONATORE***" Nome() Cognome() "- SESSO:" < SESSO > "- DATA NASCITA:" Data() "- STATO NASCITA:" < IDENTIFICATOR >  CodiceFiscale() Indirizzo() [ "- TELEFONO FISSO:" Telefono() ] [ "- TELEFONO CELLULARE:" Telefono() ] [ EMail() ] "- CITTADINANZA:" < IDENTIFICATOR > "- GRUPPO SANGUIGNO:" < GRUPPOSANGUIGNO > "- RH:" < RH > "- DATA ISCRIZIONE:" Data() "- GRUPPO LOCALE:" < NUM > "- TESSERA GRUPPO LOCALE:" < NUM > NumeroDonazioni() Idoneo() DataProssimaDonazione() TipoDonatore() "- SEZIONE:" Sezione() ";"}void Donazione() :{}{  "***DONAZIONE***" DatiDonatore() "- DATA DONAZIONE:" Data() TipoDonazione() "- UNITA' RACCOLTA:" < UNITARACCOLTA > "- COMUNE DONAZIONE:" < IDENTIFICATOR > "- ASL DONAZIONE:"  (    < IDENTIFICATOR >  | < NUM >  )*  "- PUNTO PRELIEVO:" < NUM > ";"}void Indirizzo() :{}{  "- INDIRIZZO:" < IDENTIFICATOR >  (    < IDENTIFICATOR >  | < NUM >  )*  "- CAP:" < NUM > "- COMUNE:" < IDENTIFICATOR > [ "- STATO:" < IDENTIFICATOR > ]}void Sezione() :{}{  < IDENTIFICATOR > < NUM >
}void CodiceFiscale() :{}{  "- CODICE FISCALE:"  (    < IDENTIFICATOR >  | < NUM >  )*}void Telefono() :{}{  < NUM > [ "/" < NUM > ]}void EMail() :{}{  "- EMAIL:" < EMAIL >}void Idoneo() :{}{  "- IDONEO:" < IDONEITA >}void DataProssimaDonazione() :{}{  "- PROSSIMA DATA DISPONIBILE PER DONAZIONE:" Data()}void TipoDonatore() :{}{  "- TIPOLOGIA DONATORE:" < TIPO >}void TipoDonazione() :{}{  "- TIPO DI DONAZIONE:" < TIPODONAZIONE >}void DatiDonatore() :{}{  "- DONATORE:"  (    Nome() "-" Cognome()   | CodiceFiscale()  )}void NumeroDonazioni() :{}{  "- NUMERO DI DONAZIONI:" < NUM >}void Nome() :{}{  (    "Nome:"  | "- NOME:"  )  < IDENTIFICATOR >}void Cognome() :{}{  (    "Cognome:"  | "- COGNOME:"  )  < IDENTIFICATOR >}void Data() :{}{  < NUM > "/" < NUM > "/" < NUM >}
